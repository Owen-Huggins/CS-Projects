<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="BOTTOM_CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" style="-fx-background-image: url('file:///C:/Users/OwenS/OneDrive/Desktop/CustomizationScreen.jpeg');
-fx-background-repeat: no-repeat; -fx-background-size: 1920 1080; -fx-background-position: center center;"
      xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="cs2340.gamescreen.StartController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" prefHeight="65.0" prefWidth="272.0" text="Start Game">
      <font>
         <Font size="40.0" />
      </font></Button>
   <Button mnemonicParsing="false" onAction="#onCloseButtonClick" prefHeight="75.0" prefWidth="274.0" text="Quit Game">
      <VBox.margin>
         <Insets />
      </VBox.margin>
      <font>
         <Font size="40.0" />
      </font>
   </Button>
</VBox>
